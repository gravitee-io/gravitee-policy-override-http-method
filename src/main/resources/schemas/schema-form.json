{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "object",
    "additionalProperties": false,
    "properties": {
        "method": {
            "title": "HTTP Method",
            "description": "Override the HTTP request method with the given value.",
            "type": "string",
            "default": "GET",
            "enum": ["GET", "POST", "PUT", "DELETE", "PATCH", "HEAD", "CONNECT", "OPTIONS", "TRACE"]
        }
    },
    "required": ["method"]
}
